{
  "name": "zscanner-backend",
  "description": "zScanner backend server. For or extend for your particular needs.",
  "version": "0.9.9",
  "main": "out/src/app.js",
  "typings": "out/src/app.d.ts",
  "author": "Institute for Clinical and Experimental Medicine",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build-ts": "rm -rf out && npx tsc",
    "start": "npm run serve",
    "serve": "node out/src/index",
    "tslint-fix": "npx tslint --project . --fix",
    "tslint": "npx tslint --project .",
    "ts-start": "npx ts-node src/index",
    "ts-demo": "env ZSCANNER_AUTHENTICATOR=noop ZSCANNER_STORAGE=demo npx ts-node src/index",
    "test": "npx jest --coverage",
    "prepare": "npm run build-ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx tslint --project ."
    }
  },
  "files": [
    "out/"
  ],
  "dependencies": {
    "debug": "^2.6.3",
    "fresh": "^0.5.2",
    "koa": "^2.2.0",
    "koa-body": "^4.0.6",
    "koa-bodyparser": "^3.2.0",
    "koa-convert": "^1.2.0",
    "koa-json": "^2.0.2",
    "koa-onerror": "^4.1.0",
    "koa-router": "^7.1.1",
    "koa-static": "^3.0.0",
    "koa-views": "^5.2.1",
    "koa2": "^2.0.0-alpha.7",
    "koa2-formidable": "^1.0.2",
    "minimist": "^1.2.0",
    "moment": "^2.23.0",
    "pug": "^2.0.0-rc.1",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "uuid": "^3.3.2",
    "winston": "^3.2.1",
    "handlebars": ">=4.3.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.4",
    "@types/fresh": "^0.5.0",
    "@types/jest": "^24.0.15",
    "@types/koa": "^2.0.49",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-json": "^2.0.18",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-router": "^7.0.42",
    "@types/minimist": "^1.2.0",
    "@types/mongoose": "^5.5.7",
    "@types/pug": "^2.0.4",
    "@types/request": "^2.48.1",
    "@types/request-promise": "^4.1.44",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^3.4.5",
    "husky": "^3.0.0",
    "jest": "^24.8.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.5.2",
    "winston-null": "^2.0.0"
  }
}
